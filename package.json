{
  "name": "@bydeusz/agent-md-docs",
  "version": "1.0.1",
  "description": "A CLI tool that downloads framework documentation and generates a minified docs index for AGENTS.md or CLAUDE.md",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "agent-md-docs": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/index.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "agents",
    "claude",
    "copilot",
    "cursor",
    "docs",
    "documentation",
    "cli",
    "react",
    "nextjs",
    "nestjs",
    "django",
    "fastapi",
    "django-rest-framework",
    "express",
    "agents-md",
    "claude-md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "dependencies": {
    "@inquirer/prompts": "^7.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.0.0"
  }
}
